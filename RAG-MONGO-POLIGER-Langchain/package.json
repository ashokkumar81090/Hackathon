{
  "name": "rag-mongo-poliger-langchain",
  "version": "1.0.0",
  "description": "RAG system for IT Incidents using LangChain, OpenAI Embeddings, and MongoDB Atlas Vector Search",
  "type": "module",
  "main": "dist/server.js",
  "engines": {
    "node": ">=18.17"
  },
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "tsx watch src/server.ts",
    "client": "cd client && npm start",
    "build": "tsc -p tsconfig.json && cd client && npm run build",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "ingest": "tsx src/scripts/ingestIncidents.ts",
    "ingest:clear": "tsx src/scripts/ingestIncidents.ts --clear",
    "create-index": "tsx src/scripts/createVectorIndex.ts",
    "test:search": "tsx src/scripts/testSearch.ts",
    "debug:search": "tsx src/scripts/debugSearch.ts",
    "test:bm25": "tsx src/scripts/testBM25.ts",
    "check:bm25": "tsx src/scripts/checkBM25Index.ts",
    "test:vector": "tsx src/scripts/testVectorSearch.ts",
    "test:filters": "tsx src/scripts/testVectorFilters.ts",
    "fetch:servicenow": "tsx src/scripts/fetchServiceNowData.ts",
    "generate:incidents": "tsx src/scripts/generateIncidents.ts",
    "transform:incidents": "tsx src/scripts/transformIncidents.ts",
    "clear:incidents": "tsx src/scripts/clearIncidents.ts",
    "test:mongodb": "tsx src/scripts/testMongoDB.ts",
    "verify:bm25": "tsx src/scripts/verifyBM25Index.ts",
    "inspect:doc": "tsx src/scripts/inspectDocument.ts",
    "convert:excel": "tsx src/scripts/convertToExcel.ts",
    "convert:json": "tsx src/scripts/convertExcelToJson.ts",
    "postinstall": "cd client && npm install"
  },
  "keywords": [
    "RAG",
    "LangChain",
    "MongoDB",
    "Vector Search",
    "Incidents",
    "OpenAI",
    "TypeScript"
  ],
  "dependencies": {
    "@langchain/anthropic": "^0.3.8",
    "@langchain/community": "^0.3.16",
    "@langchain/core": "^0.3.22",
    "@langchain/groq": "^0.1.3",
    "@langchain/mistralai": "^0.2.3",
    "@langchain/mongodb": "^0.1.0",
    "@langchain/openai": "^0.3.15",
    "axios": "^1.13.2",
    "dotenv": "^16.4.7",
    "express": "^4.21.1",
    "mongodb": "^6.10.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.4",
    "concurrently": "^9.2.1",
    "tsx": "^4.16.2",
    "typescript": "^5.6.3"
  }
}
